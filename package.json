{
  "name": "react-template",
  "description": "React developement template based on vite. Includes rsuite, eslint, prettier, react-router, less, json5, husky, conventional-changelog.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "lint": "eslint ./src/**/*.{js,jsx} --max-warnings 0",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "rsuite": "^5.4.0"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^1.3.5",
    "@vitejs/plugin-react": "^1.0.7",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "prettier": "^2.5.1",
    "vite": "^2.7.2"
  }
}
